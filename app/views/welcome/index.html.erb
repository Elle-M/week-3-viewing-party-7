<%= button_to "Create New User", register_path, method: :get %>

<section class="log-out">    
  <% if current_user %>
    <p><%= current_user.email %></p>
    <%= link_to "Log Out", logout_path, method: :delete %>
  <% else %>
    <%= link_to "Log In", login_path %>
  <% end %>
</section>

<section class="existing-users">
  <% if current_user %>
    <h3>Existing Users:</h3>
      <% user.each do |user| %>
        <%= user.email %>
      <% end %>
  <% end %>    
</section>   


